{
  "name": "@copper/plugin-timestream",
  "version": "1.0.0",
  "description": "Architect (arc.codes) serverless framework plugin that defines AWS Timestream databases and tables",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "test:unit": "jasmine --config=test/jasmine.json",
    "test": "npm run lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copperinc/plugin-timestream.git"
  },
  "keywords": [
    "serverless",
    "arc",
    "cloudformation",
    "timestream"
  ],
  "author": "Copper Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/copperinc/plugin-timestream/issues"
  },
  "homepage": "https://github.com/copperinc/plugin-timestream#readme",
  "devDependencies": {
    "@architect/eslint-config": "^1.0.0",
    "@architect/inventory": "^1.3.0-RC.1",
    "eslint": "^7.19.0",
    "fs-extra": "^10.0.0",
    "jasmine": "^3.6.4"
  },
  "dependencies": {
    "@architect/package": "^6.2.0-RC.1",
    "@architect/sandbox": "^3.4.0-RC.2"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config",
    "rules": {
      "brace-style": "off",
      "indent": [
        "error",
        4
      ],
      "no-magic-numbers": "off",
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
